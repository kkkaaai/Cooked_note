{
  "expo": {
    "name": "CookedNote",
    "slug": "cookednote",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "cookednote",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.cookednote.app",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cookednote.app"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf",
      "expo-sqlite"
    ]
  }
}
